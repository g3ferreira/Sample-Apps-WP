@model IEnumerable<_8pos.Models.Entity.PosUser>
@section css{
    <link rel="stylesheet" href="~/Content/Css/details.css" />
    <link rel="stylesheet" href="~/Content/Css/create.css" />
    <link rel="stylesheet" href="~/Content/Css/colors.css" />
    <link rel="stylesheet" href="~/Content/Css/flaticon.css" />
}
<div class="detail">
    <div class="container">
        <table class="table">
            <tr>
                <th>
                    User ID
                    <!--@Html.DisplayNameFor(model => model.PosUserId)-->
                </th>
                <th>
                    First Name

                    <!--@Html.DisplayNameFor(model => model.FirstName)-->
                </th>
                <th>
                    Last Name
                    <!-- @Html.DisplayNameFor(model => model.LastName)-->
                </th>
                <th>
                    User Name
                    <!--@Html.DisplayNameFor(model => model.UserName)-->
                </th>
                <th>
                    Email
                    <!-- @Html.DisplayNameFor(model => model.Email)-->
                </th>
                <th>
                    Password
                    <!-- @Html.DisplayNameFor(model => model.Password)-->
                </th>
                <!--
                <th>
                    @Html.DisplayNameFor(model => model.ActivationToken)
                </th>-->
                <th>
                    @Html.DisplayNameFor(model => model.Status.Type)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.PosUserId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FirstName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.LastName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Password)
                    </td>
                    <!--<td>
                        @Html.DisplayFor(modelItem => item.ActivationToken)
                    </td>-->
                    <td>
                        @Html.DisplayFor(modelItem => item.Status.Type)
                    </td>

                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.PosUserId }) |
                        @Html.ActionLink("Details", "Details", new { id = item.PosUserId }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.PosUserId })
                    </td>
                </tr>
            }
        </table>
        <p>
            @Html.ActionLink("Create New User", "Create")
        </p>
    </div>
</div>
